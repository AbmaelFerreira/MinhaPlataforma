{% extends 'base.html.twig' %}

 {% block body %}
    <h1> Reportagens </h1>

     <main style="max-width: 330px" class="form-signin w-100 m-auto">
         <form method="post">
             <img class="mb-4" src="/docs/5.3/assets/brand/bootstrap-logo.svg" alt="" width="72" height="57">
             <h1 class="h3 mb-3 fw-normal">Please sign in</h1>

             <div style="margin: 5px;" class="form-floating">
                 <input type="text" name="nome" class="form-control" id="nome" placeholder="name">
                 <label for="floatingInput">Nome</label>
             </div>

             <div  style="margin: 5px;" class="form-floating">
                 <input type="text" name="sobrenome" class="form-control" id="sobrenome" placeholder="sobrename">
                 <label for="floatingInput">Sobre Nome</label>
             </div>


             <button  style="margin: 5px;" class="w-100 btn btn-lg btn-primary" type="submit">Enviar</button>
             <p class="mt-5 mb-3 text-body-secondary">© 2017–2023</p>
         </form>
     </main>
 {% endblock %}